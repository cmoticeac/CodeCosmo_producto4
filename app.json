{
  "expo": {
    "name": "CodeCosmo_producto4",
    "slug": "CodeCosmo_producto4",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "platforms": ["android", "ios", "web"],
    "android": {
      "adaptiveIcon": {
        "googleServicesFile": "./google-services.json",
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "android": {
        "googleServicesFile": "./google-services.json",
        "adaptiveIcon": {
          "foregroundImage": "./assets/images/adaptive-icon.png",
          "backgroundColor": "#ffffff"
        }
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png",
      "use": "minimal-ui",
      "vapidPublicKey": "BMnQLNXZ3i-Y-2QlJVdKSlQHF9yMCTF1uHAB9PbKlw--s_GUChAYpKasx-5A7x_Zu-meyYv8iboc8QSFqhJoTGA"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
